<%= form_with model: @event do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %><br>
    <%= form.label :place %>
    <%= form.text_field :place %><br>
    <%= form.label :date %>
    <%= form.date_field :date %><br>
    <%= form.label :time %>
    <%= form.time_field :time %><br>
    <%= form.submit %>
  <% end %>